{
  "title":"Prescription",
  "type":"object",
  "properties":{
    "identifier":{
      "type":"string",
      "minLength":1
    },
    "shape":{
      "enum":[
        "Round",
        "Triangular",
        "Oblong",
        "Drop",
        "Oval",
        "Other"
      ]
    },
    "name":{
      "type":"string",
      "minLength":1
    },
    "storage":{
      "enum":[1,2,3,4]
    },
    "dimensions":{
      "title":"Dimensions",
      "type": "object",
      "properties": {
        "length":{
          "type":"number",
          "exclusiveMinimum":true,
          "minimum":0
        },
        "width":{
          "type":"number",
          "exclusiveMinimum":true,
          "minimum":0
        },
        "height":{
          "type":"number",
          "exclusiveMinimum":true,
          "minimum":0
        }
      },
      "required":[
        "width",
        "height",
        "length"
      ]
    },
    "quantity":{
      "type":"integer",
      "exclusiveMinimum":true,
      "minimum":0
    }
  },
  "required":[
    "identifier",
    "shape",
    "name",
    "storage",
    "dimensions",
    "quantity"
  ]
}
